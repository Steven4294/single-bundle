{
  "name": "cartwheel-elem",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --extra-webpack-config webpack.serve.extra.js  -o",
    "build": "ng build --extra-webpack-config webpack.externals.js --single-bundle true --prod",
    "start:prod": "live-server dist/elements-loading",
    "build:modern": "ng build --plugin ngx-build-modern --project empty-demo --prod",
    "build:plugin": "tsc hook/modern-plugin --module commonjs --target es5 --lib DOM,ES6,ScriptHost && ng build --plugin ~hook/modern-plugin",
    "build:test": "tsc test.ts --module commonjs --target es5 --lib DOM,ES6,ScriptHost && node test.js",
    "build:externals": "ng build --extra-webpack-config webpack.externals.js --prod --single-bundle",
    "start:externals": "ng serve --extra-webpack-config webpack.externals.js --single-bundle true -o",
    "build:externals:empty-demo": "ng build --extra-webpack-config webpack.externals.js --prod --project empty-demo",
    "start:externals:empty-demo": "ng serve --extra-webpack-config webpack.externals.js --project empty-demo",
    "build:externals:elements-loading": "ng build --extra-webpack-config webpack.externals.js --prod --project elements-loading",
    "start:externals:elements-loading": "ng serve --extra-webpack-config webpack.externals.js --project elements-loading",
    "build:old": "ng build --extra-webpack-config webpack.externals.js --single-bundle true --prod",
    "start:old": "ng serve --extra-webpack-config webpack.externals.js --single-bundle true -o",
    "build:elements-loading": "ng build --extra-webpack-config webpack.externals.js --single-bundle true --prod --project elements-loading",
    "start:elements-loading": "ng serve --extra-webpack-config webpack.externals.js --single-bundle true --project elements-loading -o",
    "npx-build-plus:copy-assets": "node projects/externals-demo/copy-wc-polyfill.js && node copy-wc-polyfill.js && node copy-bundles.js && node projects/externals-demo/copy-bundles.js && node projects/delme1/copy-bundles.js && node projects/delme1-host/copy-bundles.js && node projects/delme2-host/copy-bundles.js && node projects/delme2/copy-bundles.js && node projects/empty-demo/copy-wc-polyfill.js && node projects/externals-demo-scripts//copy-wc-polyfill.js",
    "build:externals-demo": "ng build --extra-webpack-config webpack.externals.js --single-bundle true --prod --project externals-demo",
    "start:externals-demo": "ng serve --extra-webpack-config webpack.externals.js --single-bundle true --project externals-demo -o",
    "build:delme1:externals": "ng build --extra-webpack-config webpack.externals.js --single-bundle true --prod --project delme1",
    "build:delme1-host:externals": "ng build --extra-webpack-config webpack.externals.js --single-bundle true --prod --project delme1-host",
    "build:delme2-host:externals": "ng build --extra-webpack-config webpack.externals.js  --prod --project delme2-host",
    "build:delme2:externals": "ng build --extra-webpack-config webpack.externals.js -- single-bundle --output-hashing none --prod --project delme2",
    "build:elements-loading:externals": "ng build --extra-webpack-config webpack.externals.js --prod --project elements-loading --single-bundle",
    "build:externals-demo-scripts:externals": "ng build --extra-webpack-config projects/externals-demo-scripts/webpack.externals.js --prod --project externals-demo-scripts --single-bundle"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/animations": "~11.2.7",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/elements": "^12.1.2",
    "@angular/forms": "~11.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "app": "^0.1.0",
    "core-js": "^3.15.2",
    "cpr": "^3.0.1",
    "document-register-element": "^1.14.10",
    "domino": "^2.1.6",
    "live-server": "^1.2.1",
    "ng-packagr": "^12.1.2",
    "ngx-build-modern": "^1.1.10",
    "ngx-build-plus": "^11.0.0",
    "rxjs": "~6.6.0",
    "tsickle": "^0.43.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.14",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "^7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
